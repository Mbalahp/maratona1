namespace exer20
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void ButtonExecutar_Click(object sender, EventArgs e)
        {
            textBoxResultado.Clear();
            string resultado = "";

            for (int i = 1; i <= 10; i++)
            {
                resultado += i;

                for (int j = 1; j <= 10; j++)
                {
                    resultado += ", " + j;
                }

                resultado += Environment.NewLine;
            }

            textBoxResultado.Text = resultado;
        }
    }
}
